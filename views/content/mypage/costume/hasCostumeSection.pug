- const costumelistArr = self.bodyData.rows
- console.log('costumelistArr: ', costumelistArr);

.board-hdr

  .label-box._fb.flex._c
    i.icon-title-b.fas.fa-list.fa-fw.c-blue
    span: |所持コスプレ衣装一覧

.board-cnt.list-board
  if(costumelistArr && costumelistArr.length)
    ul.st_options#has_coutume_list
      - for (let cl of costumelistArr)
        li.label-icon-link
          .link-wrap.flex._c
            .st-string-set
              span.sub-text: |#{cl.content_title_name}
              p: |#{cl.content_chara_name}
              input(type="hidden", name="has_chara_item", data-costume_id=cl.costume_id)
            i.icon-link.fas.fa-chevron-right.fa-fw
  
  else
    .no_list_box
      p: |所持コスプレ衣装はありません。
      
