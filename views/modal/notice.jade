-
  const notice_list_item = self.bodyData.notice_list_item;
  const notice_list_pager = self.bodyData.notice_list_pager;
  console.log(notice_list_item);
  console.log(notice_list_pager);
-


form(action="", name="noticeForm")

  .board-prm

    .board-cnt._s
      - if(notice_list_item){
      - for(var notice of notice_list_item){
      .card-prm.notice-box
        .card-hdr.notice-hdr(data-noticeid="#{notice.id}", data-type="#{notice.type}")
          .notice-data-type-set
            span.notice-type: |#{notice.type}
            span.notice-date: |#{notice.date_info.year}年#{notice.date_info.month}月#{notice.date_info.day}日
          p.notice-title: |#{notice.title}
        .card-cnt
          article.notice-cnt
            |#{notice.content}
          .btn-notice-read: |さらに読み込む
      -}
      - }
      

    .board-ftr._s
      - if(notice_list_pager){
      .pager#noticePager
        ul.flex._jc
          -if(notice_list_pager.now_page > 1){
            li: span(data-page="1"): i.icon-pager.fas.fa-angle-double-left.fa-fw
            li: span(data-page="#{notice_list_pager.now_page-1}"): i.icon-pager.fas.fa-angle-left.fa-fw
          -}
          - for(let i=0;i<notice_list_pager.disp_page_list.length;i++){ let p = notice_list_pager.disp_page_list[i];
            li: span(class="#{p == notice_list_pager.now_page ? 'selected' : '' }", data-page="#{p}"): |#{p}
          - }
          -if(notice_list_pager.now_page < notice_list_pager.max_page){
            li: span(data-page="#{notice_list_pager.now_page-0+1}"): i.icon-pager.fas.fa-angle-right.fa-fw
            li: span(data-page="#{notice_list_pager.max_page}"): i.icon-pager.fas.fa-angle-double-right.fa-fw
          -}
      - }
      
            