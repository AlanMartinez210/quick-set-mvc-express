{
  "name": "c2link",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development node ./bin/www",
    "start-node": "NODE_ENV=development nodemon ./bin/www",
    "pre-start-node": "nodemon ./bin/www",
    "start-node-win": "set NODE_ENV=development&&nodemon ./bin/www",
    "pm2": "pm2",
    "pm2:start": "NODE_ENV=development pm2 start app.json --watch && pm2 logs",
    "pm2:stop": "pm2 stop all",
    "pm2:delete": "pm2 delete all",
    "webpack": "webpack --watch",
    "dev": "pm2 start app.json --watch && webpack --watch",
    "test": "mocha --recursive --timeout 1000000 --exit",
    "test:h": "mocha --recursive --timeout 10000 test/common/helper/test-*.js",
    "test:c": "mocha --recursive --timeout 10000 test/controller/test-*.js",
    "test:s": "mocha --recursive --timeout 10000 test/services/test-*.js",
    "test:r": "mocha --recursive --timeout 10000 test/repository/test-*.js",
    "test:m": "mocha --recursive --timeout 10000 test/common/middleware/test-*.js",
    "test:x": "mocha --recursive --timeout 10000 -- ",
    "ptn": "node testdata/pattern/patternChecker.js",
    "sequelize": "node_modules/.bin/sequelize --",
    "db:create": "node_modules/.bin/sequelize model:generate",
    "migrate:up": "node_modules/.bin/sequelize db:migrate",
    "migrate:down": "node_modules/.bin/sequelize db:migrate:undo:all",
    "migrate:reset": "(node_modules/.bin/sequelize db:drop || node_modules/.bin/sequelize db:create) && node_modules/.bin/sequelize db:migrate",
    "seed:gen:user": "node_modules/.bin/sequelize seed:generate --name test-user",
    "seed:gen:schedule": "node_modules/.bin/sequelize seed:generate --name test-schedule",
    "seed:gen:tag": "node_modules/.bin/sequelize seed:generate --name test-tag",
    "seed:run": "node_modules/.bin/sequelize db:seed:all",
    "seed:undo": "node_modules/.bin/sequelize db:seed:undo:all"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "config": "^1.30.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "crypto-js": "^3.1.9-1",
    "debug": "~3.1.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-validator": "^5.2.0",
    "jade": "~1.11.0",
    "jimp": "^0.2.28",
    "jp-prefecture": "^0.2.1",
    "jquery": "^3.3.1",
    "jquery-ui": "^1.12.1",
    "jquery-ui-bundle": "^1.12.1-migrate",
    "koyomi": "^0.5.1",
    "moment-timezone": "^0.5.17",
    "morgan": "~1.9.0",
    "multer": "^1.3.0",
    "mysql2": "^1.5.3",
    "npm": "^6.4.1",
    "passport": "^0.4.0",
    "passport-twitter": "^1.0.4",
    "pm2": "^2.10.4",
    "sequelize": "^4.37.10",
    "sequelize-cli": "^4.0.0",
    "serve-favicon": "~2.5.0",
    "slick-carousel": "^1.8.1",
    "socket.io": "^2.1.1",
    "toml": "^2.3.3",
    "url-loader": "^1.0.1",
    "validator": "^10.2.0",
    "webpack": "^4.10.0",
    "webpack-cli": "^2.1.4",
    "webpack-jquery-ui": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chai-datetime": "^1.5.0",
    "chai-moment": "^0.1.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "faker": "^4.1.0",
    "husky": "^1.0.0-rc.13",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.2",
    "node-watch": "^0.5.8",
    "nodemon": "^1.17.5",
    "sass-loader": "^7.0.1",
    "sinon": "^6.3.3",
    "style-loader": "^0.21.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
